{
    "builds": [{
        "src": "salesapp/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "salesapp/wsgi.py"
        }
    ],
    "functions": {
    "app/api/**/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "env": {
    "MY_API_KEY": "your-key"
  }
}

